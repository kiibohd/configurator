{
  "name": "kiibohd-configurator",
  "version": "1.1.0",
  "description": "Configuration utility for Input Club keyboards",
  "author": "Jeremy Bondeson <jbondeson@gmail.com> (https://input.club)",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "rebuild-deps": "electron-builder install-app-deps",
    "lint": "eslint src/",
    "validate-typescript": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiibohd/configurator"
  },
  "electronWebpack": {
    "title": true,
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.32",
    "@mdi/font": "^4.5.95",
    "bluebird": "^3.7.1",
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "command-exists": "^1.2.8",
    "compare-versions": "^3.5.1",
    "he": "^1.2.0",
    "jszip": "^3.2.2",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "loglevel": "^1.6.6",
    "mdi-material-ui": "^6.8.0",
    "mkdirp": "^0.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-color": "^2.17.3",
    "react-dom": "^16.12.0",
    "source-map-support": "^0.5.16",
    "url-join": "^4.0.1",
    "usb": "^1.6.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.7.0",
    "@types/bluebird": "^3.5.29",
    "@types/classnames": "^2.2.9",
    "@types/command-exists": "^1.2.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/he": "^1.1.0",
    "@types/jszip": "^3.1.6",
    "@types/lodash": "^4.14.148",
    "@types/mkdirp": "^0.5.2",
    "@types/react": "^16.9.11",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.4",
    "@types/react-transition-group": "^4.2.3",
    "@types/url-join": "^4.0.0",
    "@types/usb": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "electron": "^7.1.1",
    "electron-builder": "22.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-webpack": "^2.7.4",
    "electron-webpack-eslint": "^5.0.1",
    "electron-webpack-ts": "^3.2.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "^4.41.2"
  },
  "resolutions": {
    "upath": "^1.2.0"
  },
  "build": {
    "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
    "buildDependenciesFromSource": true,
    "productName": "Kiibohd Configurator",
    "appId": "club.input.KiibohdConfigurator",
    "asar": true,
    "nodeGypRebuild": false,
    "npmRebuild": true,
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "identity": null,
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "tar.gz"
      ],
      "executableName": "kiibohd-configurator",
      "vendor": "Input Club",
      "category": "Utility"
    },
    "directories": {
      "buildResources": "build",
      "output": "output"
    },
    "publish": {
      "provider": "github"
    }
  }
}
